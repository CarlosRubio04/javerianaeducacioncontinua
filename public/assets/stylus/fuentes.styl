@font-face {
    font-family: 'droid_sansbold';
    src: url('../fuentes/droidsans-bold.eot');
    src: url('../fuentes/droidsans-bold.eot?#iefix') format('embedded-opentype'),
         url('../fuentes/droidsans-bold.woff2') format('woff2'),
         url('../fuentes/droidsans-bold.woff') format('woff'),
         url('../fuentes/droidsans-bold.ttf') format('truetype'),
         url('../fuentes/droidsans-bold.svg#droid_sansbold') format('svg');
    font-weight: normal;
    font-style: normal;

}

